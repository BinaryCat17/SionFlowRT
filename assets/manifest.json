{
  "parameters": {
    "WIDTH": 800,
    "HEIGHT": 600
  },
  "programs": [
    { "id": "brush", "path": "assets/brush.json" },
    { "id": "canvas", "path": "assets/canvas.json" }
  ],
  "mappings": [
    { "program": "brush", "tensor": "mouse_pos", "source": { "type": "MousePosition" } },
    { "program": "brush", "tensor": "uv", "source": { "type": "ScreenUV" } },
    { "program": "brush", "tensor": "time", "source": { "type": "Time" } },
    { "program": "canvas", "tensor": "brush_input", "source": { "type": "Link", "program": "brush", "output": "output" } },
    { "program": "canvas", "tensor": "prev_frame", "source": { "type": "Link", "program": "canvas", "output": "final_output" } },
    { "program": "canvas", "tensor": "final_output", "source": { "type": "Display" } }
  ]
}